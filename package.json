{
  "name": "obsidian-tag-agent",
  "version": "1.0.0",
  "description": "An AI-powered Obsidian plugin using LangChain.js with Ollama for local tag management",
  "main": "main.js",
  "scripts": {
    "dev": "node -r ./.node-warning-suppression.js esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node -r ./.node-warning-suppression.js esbuild.config.mjs production",
    "build:types": "tsc -noEmit -skipLibCheck",
    "build:only": "node -r ./.node-warning-suppression.js esbuild.config.mjs production",
    "version": "node -r ./.node-warning-suppression.js version-bump.mjs",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:integration": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js --selectProjects integration",
    "test:e2e": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js --selectProjects e2e",
    "test:watch": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:integration:watch": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js --selectProjects integration --watch",
    "test:e2e:watch": "node -r ./.node-warning-suppression.js --experimental-vm-modules node_modules/jest/bin/jest.js --selectProjects e2e --watch"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "langchain",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "0.24.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "obsidian": "latest",
    "prettier": "^3.3.3",
    "stream-web": "^0.5.2",
    "ts-jest": "^29.2.5",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "web-streams-polyfill": "^4.0.0"
  },
  "dependencies": {
    "@langchain/community": "^0.3.15",
    "@langchain/core": "^0.3.18",
    "langchain": "^0.3.6"
  }
}
