{
  "name": "obsidian-tag-agent",
  "version": "1.0.0",
  "description": "An AI-powered Obsidian plugin using LangChain.js with Ollama for local tag management",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:types": "tsc -noEmit -skipLibCheck",
    "build:only": "node esbuild.config.mjs production",
    "version": "node version-bump.mjs",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "jest",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:watch": "jest --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:e2e:watch": "jest --selectProjects e2e --watch"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "langchain",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.7.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.14.47",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "obsidian": "latest",
    "prettier": "^3.3.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "langchain": "^0.0.200"
  }
}
