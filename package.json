{
  "name": "obsidian-insight-engine",
  "version": "0.1.0",
  "description": "AI-powered knowledge assistant using LangChain.js with Ollama for local processing",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run tests/e2e/e2e.test.ts"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "langchain",
    "ollama"
  ],
  "author": "Thomas Hochbichler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vodaza36/obsidian-insight-engine"
  },
  "bugs": {
    "url": "https://github.com/vodaza36/obsidian-insight-engine/issues"
  },
  "homepage": "https://github.com/vodaza36/obsidian-insight-engine#readme",
  "minAppVersion": "1.4.0",
  "dependencies": {
    "@langchain/core": "^0.3.18",
    "@langchain/ollama": "^0.1.2",
    "@langchain/openai": "^0.3.14",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "builtin-modules": "^4.0.0",
    "esbuild": "0.24.0",
    "obsidian": "latest",
    "typescript": "5.7.2",
    "vitest": "^1.3.1"
  }
}
