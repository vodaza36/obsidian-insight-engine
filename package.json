{
  "name": "obsidian-tag-agent",
  "version": "1.0.0",
  "description": "An AI-powered Obsidian plugin using LangChain.js with Ollama for local tag management",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run tests/e2e/e2e.test.ts"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "langchain",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.18",
    "@langchain/ollama": "^0.1.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "builtin-modules": "^4.0.0",
    "esbuild": "0.24.0",
    "obsidian": "latest",
    "typescript": "5.7.2",
    "vitest": "^1.3.1"
  }
}
