{
  "name": "obsidian-tag-agent",
  "version": "1.0.0",
  "description": "An AI-powered Obsidian plugin using LangChain.js with Ollama for local tag management",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "langchain",
    "ollama"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.7.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.14.47",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "obsidian": "latest",
    "prettier": "^3.3.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "langchain": "^0.0.200"
  }
}
